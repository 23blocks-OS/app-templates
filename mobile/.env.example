# 23blocks SDK Configuration
# Copy this file to .env and fill in your values

# Required: Your 23blocks application ID
EXPO_PUBLIC_BLOCKS_APP_ID=your-app-id

# Optional: Your tenant ID (if using multi-tenant setup)
EXPO_PUBLIC_BLOCKS_TENANT_ID=

# Required: Authentication service URL
EXPO_PUBLIC_BLOCKS_AUTH_URL=https://gateway.23blocks.com

# Optional: Additional service URLs (if different from auth URL)
# EXPO_PUBLIC_BLOCKS_SEARCH_URL=
# EXPO_PUBLIC_BLOCKS_CRM_URL=
# EXPO_PUBLIC_BLOCKS_CONTENT_URL=
